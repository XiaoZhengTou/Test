/*进度上*/
.personalCenter .tuikuan .container > .item {
  padding-top: 30px;
  position: relative;
}
.personalCenter .tuikuan .container > .item:before {
  content: '';
  position: absolute;
  left: 0;
  right: 0;
  top: 14px;
  height: 2px;
  background: #ccc;
}
.personalCenter .tuikuan .container > .item:first-of-type:before{
  right: 0;
  left: 50%;
  width: 50%;
}
.personalCenter .tuikuan .container > .item:last-of-type:before{
  right: 0;
  width: 50%;
}
.personalCenter .tuikuan .container > .item:after {
  content: '';
  position: absolute;
  left: calc(50% - 7px);
  top: 5px;
  width: 20px;
  height: 20px;
  text-align: center;
  line-height: 20px;
  border-radius: 50%;
  background: #ccc;
}
.personalCenter .tuikuan .container > .item:first-child.active:after {
  font-size: 12px;
  content: "\e67b";
  font-family: "iconfont";
  color: white;
}

/*进度下*/
.personalCenter .tuikuan .container-bot > .item {
  padding-left: 10%;
  position: relative;
}
.personalCenter .tuikuan .container-bot > .item:before {
  content: '';
  position: absolute;
  left: 0;
  width: 2px;
  height: 51%;
  background: #ccc;
}
.personalCenter .tuikuan .container-bot > .item:first-child:before{
	  top: 50%;
}
.personalCenter .tuikuan .container-bot > .item:after {
  content: '';
  position: absolute;
  width: 20px;
  height: 20px;
  border-radius: 50%;
  background: #ccc;
  left: -9px;
  top: 32%;
}
.personalCenter .tuikuan .item.active:before,
.personalCenter .tuikuan .item.active:after {
  background: #42A5F5;
}
.personalCenter .tuikuan .item.active,
.personalCenter .tuikuan .item.active{
  color: #212121;
}
/*tuikuan 退款详情完*/
