<md-dialog aria-label="组织机构" style="max-height: 600px;min-width: 400px;">
	<form>
		<md-toolbar>
			<div class="md-toolbar-tools">
				<h2>收款账户
				</h2>
				<span flex></span>
				<md-button ng-click="cancel()">
					<i class="iconfont" style="color:#fff;">关闭</i>
				</md-button>
			</div>
		</md-toolbar>
		<md-dialog-content>
			<div style="text-align: center;" class="md-padding" ng-if="banks.code!='0000'">{{banks.msg}}</div>
			<div ng-if="banks.code=='0000'" ng-cloak>
				<md-content layout="column" id="content" flex>
					<md-list-item class="md-2-line" ng-repeat="item in banks.data.dataList" ng-click="answer(item);">
						<span class="ml-circle-icon ml-margin-right" style="width: 40px;height: 40px;text-align: center;line-height: 40px;">
							<i class="iconfont">&#xe677;</i>
						</span>
						<div class="md-list-item-text" layout="column">
							<p style="font-size: 1em"> {{ item.bank_name }} </p>
							<p style="font-size: 0.8em;color:#9E9E9E;">{{ item.receiver }}【{{ item.bank_account }}】</p>
							<p style="font-size: 0.8em;color:#9E9E9E;"> {{ item.bank_adress }} </p>
						</div>
						<md-divider></md-divider>
					</md-list-item>
				</md-content>
			</div>
		</md-dialog-content>
	</form>
</md-dialog>