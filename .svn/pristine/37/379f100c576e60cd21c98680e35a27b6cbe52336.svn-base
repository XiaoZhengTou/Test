.jiekuan.add h2.title {
  font-weight: normal;
  font-size: 17px;
  line-height: 1;
  margin: 0;
  border-left: 3px solid #42A5F5;
  margin-left: -16px;
  padding-left: 16px;
}
.jiekuan.add p.emptyInfo {
  margin: 0 0 20px;
  text-align: center;
  color: #e10c35;
  font-size: 18px;
}
.jiekuan.add .cardselect .ml-card {
  padding: 0;
}
.jiekuan.add .cardselect .ml-card h3 {
  margin: 0;
  text-align: center;
  line-height: 2;
  font-size: 17px;
  border-bottom: 1px solid rgba(0, 0, 0, 0.12);
  font-weight: normal;
  background: #42A5F5;
}
.jiekuan.add .cardselect .ml-card > div {
  cursor: pointer;
  padding: 10px;
  height: 90px;
  position: relative;
}
.jiekuan.add .cardselect .ml-card > div.content:hover p {
  opacity: .2;
}
.jiekuan.add .cardselect .ml-card > div.content:hover p.del {
  opacity: 1;
  display: block;
}
.jiekuan.add .cardselect .ml-card > div p {
  margin: 0;
  line-height: 2;
  display: block;
  overflow: hidden;
  text-overflow: ellipsis;
  white-space: nowrap;
}
.jiekuan.add .cardselect .ml-card > div p.del {
  display: none;
  position: absolute;
  margin: 0;
  line-height: 1;
  top: 0;
  left: -1px;
  right: -1px;
  bottom: -1px;
  background: rgba(255, 255, 255, 0.3);
  text-align: center;
  line-height: 111px;
}
.jiekuan.add .cardselect .ml-card > div p.del i {
  font-size: 30px;
}
.jiekuan.add .cardselect .ml-card > div.empty {
  text-align: center;
}
.jiekuan.add .cardselect .ml-card > div.empty i {
  font-size: 30px;
  color: rgba(255, 255, 255, 0.3);
  display: block;
  margin-top: 10px;
}
.jiekuan.add .cardselect .ml-card > div.empty p {
  color: rgba(255, 255, 255, 0.8);
}
.huankuan.add tr.selectToggle td.emptyswith {
  width: 20%;
}
.huankuan.add tr.selectToggle td.emptyswith span.empty {
  display: block;
  width: 100%;
  border-top: 2px solid rgba(0, 0, 0, 0.12);
}
.huankuan.add tr.selectToggle td.emptyswith input {
  display: none;
  width: 100%;
  text-align: right;
  border: 1px solid rgba(0, 0, 0, 0.12);
  padding: 3px 5px;
  font-size: 17px;
  box-sizing: border-box;
}
.huankuan.add tr.selectToggle.md-selected td.emptyswith span.empty {
  display: none;
}
.huankuan.add tr.selectToggle.md-selected td.emptyswith input {
  display: block;
}
